{
  "name": "@joycostudio/v0-setup",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "version": "1.0.1",
  "description": "A v0 environment variables check dialog",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"tsup --watch\" \"ENVIRONMENT=development cd templates/basic && pnpm dev\"",
    "typecheck": "tsc --noEmit",
    "version:package": "pnpm changeset version",
    "release": "pnpm build && pnpm changeset publish",
    "lint": "eslint -c ./eslint.config.mjs . --fix --no-cache"
  },
  "author": "joyco.studio",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@eslint/js": "^9.18.0",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-decd7b8-20250118",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "tailwindcss": "^3.4.16",
    "tsup": "^8.0.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "motion": "^12.19.1",
    "tailwind-merge": "^3.3.1"
  }
}